function Character(name, species, personality, backstory, fetishes) constructor {
    self.name = name;                // e.g., "Lara"
    self.species = species;          // Reference to Species struct
    self.personality = personality;  // e.g., "shy"
    self.backstory = backstory;      // e.g., "Lara came to explore her desires."
    self.fetishes = fetishes;        // e.g., ["sensory play"]
}