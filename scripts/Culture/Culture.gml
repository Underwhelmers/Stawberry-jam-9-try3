function Culture(name, traditions, norms, rituals) constructor {
    self.name = name;                // e.g., "Tentacled Culture"
    self.traditions = traditions;    // e.g., ["pre-intercourse dance"]
    self.norms = norms;              // e.g., ["group intimacy"]
    self.rituals = rituals;          // e.g., ["scent marking"]
}